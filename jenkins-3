pipeline{
    agent any
    stages{
        stage("checkout code"){
            steps{
                sh """
                 checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/santoshnirwatla/datastore_project_jenkins.git']])
                """
                

            }
            
        }
    }
}